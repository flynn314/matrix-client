{
	"name": "flynn314/matrix-client",
	"description": "Matrix client",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": "^8.2",
		"ext-json": "*",
		"ext-fileinfo": "*",
		"guzzlehttp/guzzle": "^7.5"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"vlucas/phpdotenv": "^5.5",
		"larapack/dd": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"Flynn314\\Matrix\\": "src"
		}
	},
	"autoload-dev": {
        "psr-4": {
            "Flynn314\\Matrix\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flynn314\\Matrix\\MattermostServiceProvider"
            ]
        }
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
